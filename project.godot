; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Sticking To It"
config/version="0.1"
run/main_scene="res://RootNode.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false
config/icon="res://icon.svg"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=3840
window/size/viewport_height=2160
window/size/mode=3
window/size/resizable=false
window/size/borderless=true

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[navigation]

2d/default_cell_size=0.25
2d/default_edge_connection_margin=0.25
2d/default_link_connection_radius=1.0

[physics]

2d/run_on_separate_thread=true
common/physics_ticks_per_second=30
2d/physics_engine="GodotPhysics2D"

[rendering]

renderer/rendering_method="mobile"
anti_aliasing/quality/msaa_2d=2

[xr]

openxr/extensions/hand_tracking=false
